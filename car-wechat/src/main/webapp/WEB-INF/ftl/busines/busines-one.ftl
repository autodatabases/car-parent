<#include "../commons/header.ftl" />
	<title>商家信息</title>
		<link rel="stylesheet" type="text/css" href="${_RESOURCES}/css/busines/busines-one.css?v=${_version}"/>
		<script src="${_RESOURCES}/js/busines/busines_one.js?v=${_version}"></script>
	</head>
	<body onload="busines_one()";>
		<!-- 头部 -->
		<header class="header">
			<span class="back mui-action-back" onclick="javascript:history.back(-1);"></span>
			<h1>商家信息</h1>
		</header>
		<!-- 步骤 -->
		<section class="step">
			<span class="one">填写资质信息</span>
			<span class="two">上传资质文件</span>
			<span class="three">等待审核</span>
			<div></div>
			<i></i>
		</section>
		<!-- 信息 -->
		<form class="forms" action="" method="post">
			<ul class="busines_ulone">
				<li><label>店铺名称</label> <input type="text" placeholder="点击输入" /></li>
				<li><label>纳税人识别码</label> <input type="text" placeholder="点击输入" /></li>
				<li><label>注册地址</label> <input type="text" placeholder="点击输入" /></li>
				<li><label>注册电话</label> <input name="phone" class="phone" type="text" placeholder="点击输入" /></li>
				<li><label>开户行</label> <input type="text" placeholder="点击输入" /></li>
				<li><label>银行账号</label> <input type="text" placeholder="点击输入" /></li>
			</ul>
			<input type="button" class="btn one_btn" value="下一步"/>
		</form>
	</body>
</html>