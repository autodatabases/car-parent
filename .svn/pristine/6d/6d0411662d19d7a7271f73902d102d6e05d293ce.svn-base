<div class="tables-top">
	<p class="crumbs">
		<span>您的位置：</span><a>增值服务</a>&nbsp;》<a class="on">洗车套餐购买记录</a>
	</p>

	<!-- 切换头部 -->
	<div class="tab-top">
		<span class="on">购买记录列表</span>
	</div>
</div>
<!-- 列表 -->
<div class="tabels-main">
	<!-- 搜索 -->
	<div class="search-box clearfix">
		<ul>
			<li>
				<label>用户手机号:</label>
				<input type="text" id="userPhone" />
			</li>
            <li>
				<label>订单号:</label>
				<input type="text" id="orderNo" />
			</li>
            <li>
				<label>订单状态:</label>
				<select id="orderStatus">
			 		<option value="">请选择订单状态</option>
			 		<option value="0">待支付</option>
			 		<option value="1">支付中</option>
                    <option value="2">支付完成</option>
			 		<option value="3">支付失败</option>
				</select>
			</li>
 			<li>
				<button class="search-btn">
					<span>搜索</span>
				</button>
			</li>
		</ul>
	</div>
	<!-- 表格 -->
	<div class="tab-list">
		<table>
			<thead>
				<tr>
                    <th>用户手机号</th>
					<th>订单编号</th>
					<th>支付金额</th>
					<th>支付方式</th>
                    <th>购买次数</th>
                    <th>订单状态</th>
					<th>支付时间</th>
				</tr>
			</thead>
			<tbody>
				<!--<tr>
					<td>1</td>
					<td>黄埔</td>
					<td>2017-3-31</td>
					<td>2017-3-31</td>
					<td><a>编辑</a><a>删除</a></td>
				</tr>-->
			</tbody>
		</table>
	</div>
	<!-- 分页 -->
	<div class="page-box" id="taocan_paga"></div>
</div>
