.header{
	background: #fff;
	position: relative;
	border-bottom: 1px solid #e0e0e0;
}
.header h1{
	font-size: 0.9375rem;
	line-height: 2.75rem;
	text-align: center;
} 
.back{
	position: absolute;
	left: 0;
	top: 0;
	width:15%;
	height:100%;
	background:url(../../imgs/member/back-icon.png) no-repeat center;
	background-size:20% ;
}
/* 车库列表 */
.garage-list li{
	background: #fff;
	border-bottom: 1px solid #e0e0e0;
	border-top: 1px solid #e0e0e0;
	margin-top: 0.75rem;
	position: relative;
	padding:1.625rem 1.325rem ;
	overflow: hidden;
	font-size: 0.75rem;
}
.garage-list li i{
	display: none;
}
.garage-list li.on i{
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	width: 4.3125rem;
	height:2.1875rem;
	text-indent: 0.4rem;
	line-height: 1.3rem;
	font-size:0.75rem ;
	color: #fff;
	font-style: normal;
	background: url(../../imgs/member/tip-icon.png) no-repeat left top;
	background-size: 100%;
}
.garage-list li div{
	float: left;
}
.img-box{
	padding: 2px;
	width:2.5rem;
	height:2.5rem;
}
.img-box img{
	width:100%;
	height: 100%;
}
.text-box{
	padding: 0.25rem;
}
.text-box p{
	color: #b5b5b5;
}
.garage-list li a{
	position: absolute;
	right: 10%;
	top: 50%;
	margin-top: -0.5625rem;	
}



































