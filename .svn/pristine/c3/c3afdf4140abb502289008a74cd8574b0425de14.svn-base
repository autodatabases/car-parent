<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>登陆</title>
    <link rel="stylesheet" type="text/css" href="../css/normalize.min.css">
    <link rel="stylesheet" type="text/css" href="../js/dep/bootstrap/bootstrap.min.css">
</head>
<body>
<div class="login-box">
    <div class="row">
        <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4"></div>
        <div class="col-xs-12 col-sm-5 col-md-5 col-lg-5">
            <div class="panel panel-login">
                <div class="panel-heading">
                    <h3 class="panel-title">LOGIN</h3>
                </div>
                <form class="form" data-validator-option="{timely:2}"> 
                    <div class="panel-body">
                        <div class="form-horizontal">
                            <div class="form-group">
                                <div class="col-md-offset-1 col-md-9">
                                    <input id="userName" data-rule="required;userName;" type="text" placeholder="用户名" autocomplete="off" class="form-control input-md" tabIndex="1" >
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-offset-1 col-md-9">
                                    <input id="login_pwd"  type="password" placeholder="登录密码" autocomplete="off" class="form-control input-md" data-rule="required;" tabIndex="2" >
                                </div>
                            </div>
                            <!-- Button -->
                            <div class="form-group">
                                <div class="col-md-offset-1 col-md-9">
                                    <button type="submit" id="login_sub" name="login_sub" class="btn btn-danger btn-lg btn-block " tabIndex="3">登录</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3"></div>
    </div>
</div>
<script src="../js/dep/seajs/sea.js"></script>
<script src="../js/dep/seajs/sea-debug.js"></script>
<script src="../js/dep/seajs/seajs-global-config.js"></script>

<script type="text/javascript">
    seajs.use("js/member/login", function(callFn) {
        callFn.addEvent;
    }); 
</script>
</body>
</html>