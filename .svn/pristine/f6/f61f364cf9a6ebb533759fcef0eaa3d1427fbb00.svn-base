html,body {
	background-color: #fff;
}
/*头部*/
.header{
	background: #fff;
	border-bottom:1px solid #cecece;
	text-align: center;
	position: relative;
	background-color: #ff3e5b;
	color: #fff;
}
.header h1{
	height:2.75rem;
	font-size: 0.9375rem;
	line-height: 2.75rem;
}
.back{
	position: absolute;
	left: 0;
	top: 0;
	width:15%;
	height:100%;
	background: url(../../imgs/dot/back-fff.png) no-repeat center;
	background-size:20% ;
}
.content {
	width: 100%;
}
.list li{
	width: 100%;
	height: 3.2rem;
	border-bottom: 1px solid #999999;
}
.list li div {
	width: 90%;
	margin: 0 auto;
	overflow: hidden;
}
.list li div span {
	display: block;
}
.list li .detail {
	float: left;
	display: block;
	width: 70%;
	padding-top: 0.2rem;
}
.list li .detail .pointType {
	font-size: 16px;
}
.list li .detail .pointData {
	font-size: 12px;
	color: #989898;
}
.list li .number {
	display: block;
	width: 30%;
	float: left;
	text-align: right;
	line-height: 3.2rem;
	font-size: 16px;
}
.nullTips {
	background: url(../../imgs/counterman/points_tips.png) 0 0 no-repeat;
	background-size: 100% 100%;
	width: 50%;
	margin: 8rem auto;
	height: 4rem;
	display: none;
	line-height: 16rem;
	text-align: center;
	color: #8d8d8d;
}
