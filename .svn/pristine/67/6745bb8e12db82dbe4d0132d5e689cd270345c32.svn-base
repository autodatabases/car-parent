.car>span{
    position: absolute;
    right: 1rem;
    top: 50%;
    margin-top: -1rem;
    width: 4.8375rem;
    height: 1.75rem;
    line-height: 1.75rem;
    font-size: 0.75rem;
    color: #ff3e5b;
    border: 1px solid #ff3e5b;
    border-radius: 5px;
    background: #fff;
    outline: none;
    padding: 0;
    z-index: 20;
    text-align: center;
}
