package com.emate.shop.business.vo.gyvo;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name = "accsegment")
public class Accsegment {

    private Acc acc;

    public Accsegment(Acc acc) {
        super();
        this.acc = acc;
    }

    public Accsegment() {
        super();
    }

    @XmlElement(name = "acc")
    public Acc getAcc() {
        return acc;
    }

    public void setAcc(Acc acc) {
        this.acc = acc;
    }

}
