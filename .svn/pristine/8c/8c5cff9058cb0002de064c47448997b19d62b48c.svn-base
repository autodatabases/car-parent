<link rel="stylesheet" type="text/css" href="resources/css/counterman/area_edit.css?t=1.0"/>
<script src="resources/js/counterman/area_edit.js?t=1.0"></script>
<div class="tables-top">
	<p class="crumbs"><span>您的位置：</span><a>业务员</a>&nbsp;》<a class="on">财险职场列表</a></p>
</div>
<!-- 列表 -->
<div class="tabels-main">
	<dl>
		<dt><span>职场编辑</span></dt>
		<dd>
			<label>职场名称：</label><input type="text" id="careerName" value="" placeholder="请输入职场名称"/>
		</dd>
		<dd>
			<label>职场代码：</label><input type="text" id="careerCode" value="" placeholder="请输入职场代码"/>
