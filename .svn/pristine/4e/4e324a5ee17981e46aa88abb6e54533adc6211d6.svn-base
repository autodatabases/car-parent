/* 表单 */
.tables{
	float: left;
	width: 90%;
	min-height:500px;
}
.tables>div{
	border-left: 1px solid #ccc;
}
.tables-top{
	padding-top: 20px;
    background: url(../../imgs/com/noise.white.png);
	border-bottom: 1px solid #ccc;
	min-height: 80px;
}
.tab-top{
	width: 90%;
    padding: 0 20px;
    margin-bottom: -1px;
}
.tab-top span{
	display: inline-block;
    padding: 10px 20px;
    background: #ccc;
    color: #fff;
    font-size: 14px;
    -moz-border-radius: 2px 2px 0 0;
    -webkit-border-radius: 2px 2px 0 0;
    border-radius: 2px 2px 0 0;
    border: 1px solid #ccc;
    cursor: pointer;
	position: relative;
}
.tab-top span:hover{
    color: #fff;
    background: #fc9338;
    border-bottom: 1px solid #fff;
}
.tab-top span.on{
	color: #fff;
    background: #fc9338;
    border-bottom: 1px solid #fff;
}
.tab-top span ul{
    width: 156px;
	border: 1px solid #ccc;
	overflow: hidden;
	position: absolute;
	left: -1px;
	top: 39px;
	z-index: 10000;
	background: #fcfcfc;
	text-align: center;
	font-size: 16px;
	opacity: 0.9;
}
.tab-top span ul li{
	padding: 20px 0;
	font-size: 14px;
	color: #fc9338;
}
.tab-top span ul li{
	border-bottom: 1px dashed #ccc;
}
.tab-top span ul li:last-child{
	border-bottom: 0;
}
.tab-top span ul li:hover{
	background: #fc9338;
	color: #fff;
	font-size: 16px;
}
.tab-top div{
	display: inline-block;
	cursor: pointer;
	color: #fff;
	background: #fc9338;
	width: 143px;
	height:32px;
	line-height: 34px;
	text-align: center;
	margin-top: -5px;
	margin-left: 5px;
}
/* 列表 */
.tabels-main{
	padding: 20px 20px;
	min-height: 400px;
}
.tabels-main .imgUploat a {
    margin-left: 20px;
    cursor: pointer;
    border: 1px solid #F0882C;
    background: #F0882C;
    color: #fff;
    padding: 3px 10px;
    line-height: 26px;
    border-radius: 5px;
}
.tabels-main .importUser_fail{
	float:right;
}
/* 搜索 */
.search-box{
	padding: 20px;
	border:1px solid #ccc;
}
.search-box .search-btn{
	display: inline-block;
    border: 1px solid #ccc;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    -moz-box-shadow: inset 0 1px 0 #fff;
    -webkit-box-shadow: inset 0 1px 0 #fff;
    box-shadow: inset 0 1px 0 #fff;
    font-weight: bold;
    background:#f7f7f7 url(../../imgs/sprites.png) no-repeat -10px -15px;
    color: #666;
    padding: 0;
	margin-right: 0.5rem;
}
.search-box .search-btn span{
	padding: 5px 10px;
    margin-left: 25px;
    display: block;
    background: #f7f7f7;
    -moz-box-shadow: inset 0 1px 0 #fff;
    -webkit-box-shadow: inset 0 1px 0 #fff;
    box-shadow: inset 0 1px 0 #fff;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    cursor: pointer;
}
.search-box input{
    padding:5px 10px;
    border: 1px solid #ccc;
    color: #333;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    margin-right: 20px;
}
.search-box div {
	margin-top: 0.5rem;
}
.search-box select{
	display: inline-block;
	padding:5px 10px;
    border: 1px solid #ccc;
    color: #333;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    margin-right: 20px;
	width: 4rem;
	text-align: center;
	/* margin-left: 4rem; */
	vertical-align: middle;
}
.export {
	padding: 5px 1rem;;
	border-radius: 2px;
	background: #fc9338;
	color: #FFF;
}
/* 切换列表 */
.tab-list {
	border-left: 1px solid #ddd;
}
.tab-list table{
	width: 100%;
	vertical-align: middle;
}
.tab-list thead th,.tab-list tbody td {
    padding: 7px 10px;
    border: 1px solid #ddd;
    border-left: 0;
    text-align: center;
}
.tab-list thead tr th{
	text-align: center;
	background: #fcfcfc;
}
.tab-list thead tr th:nth-child(odd){
	background: #f7f7f7;
}
.tab-list thead tr th.bh,.tab-list thead tr th.cz{
	background: #fcfcfc;
}
.tab-list tbody{
	border-bottom:1px solid #ddd;
}
.tab-list tbody th{
	border-right:1px solid #ddd;
	line-height: 30px;
}
.tab-list tbody tr td:nth-child(even){
	background: #fcfcfc;
}
.tab-list tbody td{
	vertical-align: middle;
	line-height: 24px;
}
.tab-list tbody td a{
	color: #333;
	margin: 0 10px;
}
.tab-list tbody td a:hover{
	color:#fc9338;
}
