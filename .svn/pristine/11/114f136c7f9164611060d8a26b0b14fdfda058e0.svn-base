.header{
	background: #fff;
	position: relative;
	border-bottom: 1px solid #e0e0e0;
}
.header h1{
	font-size: 0.9375rem;
	line-height: 2.75rem;
	text-align: center;
} 
.back{
	position: absolute;
	left: 0;
	top: 0;
	width:15%;
	height:100%;
	background:url(../../imgs/msg/back-icon.png) no-repeat center;
	background-size:20% ;
}
/* 消息详情 */
.main{
	min-height: 40px;
	text-align: center;
}
.main p{
	padding: 15px 10px;
	font-size: 14px;
	line-height: 22px;
	border-bottom: 1px solid #e0e0e0;
}
.main span{
	font-size: 12px;
	color: #868686;
}
