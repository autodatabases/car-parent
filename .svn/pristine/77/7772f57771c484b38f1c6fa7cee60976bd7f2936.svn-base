package com.emate.shop.business.model;

import java.math.BigDecimal;
import java.util.Date;

public class SellerReport {
    /**
     * 
     * @Table seller_report.id
     */
    private Long id;

    /**
     * 
     * @Table seller_report.seller_id
     */
    private Long sellerId;

    /**
     * 报表年份
     * @Table seller_report.year
     */
    private Integer year;

    /**
     * 报表月份
     * @Table seller_report.month
     */
    private Integer month;

    /**
     * 商家本月已结算总金额
     * @Table seller_report.total_money
     */
    private BigDecimal totalMoney;

    /**
     * 不开发票 invoice
     */
    public static final java.lang.Integer INVOICE_0 = 0;
    /**
     * 开发票 invoice
     */
    public static final java.lang.Integer INVOICE_1 = 1;
    /**
     * 是否开发票：0，不开发票，1，开发票
     * @Table seller_report.invoice
     */
    private Integer invoice;

    /**
     * 商家未结算 status
     */
    public static final java.lang.Byte STATUS_0 = 0;
    /**
     * 商家已结算 status
     */
    public static final java.lang.Byte STATUS_1 = 1;
    /**
     * 问题订单 status
     */
    public static final java.lang.Byte STATUS_2 = 2;
    /**
     * 已完成 status
     */
    public static final java.lang.Byte STATUS_3 = 3;
    /**
     * 是否结算：0，商家未结算，1，商家已结算，2，问题订单，3，已完成
     * @Table seller_report.status
     */
    private Byte status;

    /**
     * 这月已完成订单数
     * @Table seller_report.order_count
     */
    private Integer orderCount;

    /**
     * 创建时间
     * @Table seller_report.create_time
     */
    private Date createTime;

    /**
     * 更新时间
     * @Table seller_report.update_time
     */
    private Date updateTime;

    /**
    * 获取 
    * @return id
    */
    public Long getId() {
        return id;
    }

    /**
    * 设置 
    * @param id
    */
    public void setId(Long id) {
        this.id = id;
    }

    /**
    * 获取 
    * @return sellerId
    */
    public Long getSellerId() {
        return sellerId;
    }

    /**
    * 设置 
    * @param sellerId
    */
    public void setSellerId(Long sellerId) {
        this.sellerId = sellerId;
    }

    /**
    * 获取 报表年份
    * @return year
    */
    public Integer getYear() {
        return year;
    }

    /**
    * 设置 报表年份
    * @param year
    */
    public void setYear(Integer year) {
        this.year = year;
    }

    /**
    * 获取 报表月份
    * @return month
    */
    public Integer getMonth() {
        return month;
    }

    /**
    * 设置 报表月份
    * @param month
    */
    public void setMonth(Integer month) {
        this.month = month;
    }

    /**
    * 获取 商家本月已结算总金额
    * @return totalMoney
    */
    public BigDecimal getTotalMoney() {
        return totalMoney;
    }

    /**
    * 设置 商家本月已结算总金额
    * @param totalMoney
    */
    public void setTotalMoney(BigDecimal totalMoney) {
        this.totalMoney = totalMoney;
    }

    /**
    * 获取 是否开发票：0，不开发票，1，开发票
    * @return invoice
    */
    public Integer getInvoice() {
        return invoice;
    }

    /**
    * 设置 是否开发票：0，不开发票，1，开发票
    * @param invoice
    */
    public void setInvoice(Integer invoice) {
        this.invoice = invoice;
    }

    /**
    * 获取 是否结算：0，商家未结算，1，商家已结算，2，问题订单，3，已完成
    * @return status
    */
    public Byte getStatus() {
        return status;
    }

    /**
    * 设置 是否结算：0，商家未结算，1，商家已结算，2，问题订单，3，已完成
    * @param status
    */
    public void setStatus(Byte status) {
        this.status = status;
    }

    /**
    * 获取 这月已完成订单数
    * @return orderCount
    */
    public Integer getOrderCount() {
        return orderCount;
    }

    /**
    * 设置 这月已完成订单数
    * @param orderCount
    */
    public void setOrderCount(Integer orderCount) {
        this.orderCount = orderCount;
    }

    /**
    * 获取 创建时间
    * @return createTime
    */
    public Date getCreateTime() {
        return createTime;
    }

    /**
    * 设置 创建时间
    * @param createTime
    */
    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    /**
    * 获取 更新时间
    * @return updateTime
    */
    public Date getUpdateTime() {
        return updateTime;
    }

    /**
    * 设置 更新时间
    * @param updateTime
    */
    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }
}